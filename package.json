{
  "name": "PocketSync",
  "version": "0.0.1",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shadow-cljs": "^2.20.15"
  },
  "devDependencies": {
    "browser-sync": "^2.27.11",
